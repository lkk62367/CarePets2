<android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <android.support.design.widget.AppBarLayout
        android:id="@+id/app_bar_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <android.support.design.widget.CollapsingToolbarLayout
            android:id="@+id/collapsing_toolbar"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:contentScrim="?attr/colorPrimary"
            app:layout_scrollFlags="scroll|exitUntilCollapsed">

            <ImageView
                android:id="@+id/image"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleType="centerCrop"
                app:layout_collapseMode="parallax"
                app:layout_collapseParallaxMultiplier="0.7" />

            <android.support.v7.widget.Toolbar
                android:id="@+id/toolbar"
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize"
                app:layout_collapseMode="pin">


            </android.support.v7.widget.Toolbar>


        </android.support.design.widget.CollapsingToolbarLayout>

    </android.support.design.widget.AppBarLayout>

    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@android:color/white"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">

        <TableLayout
            android:id="@+id/tmall"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:columnCount="1"
            android:orientation="vertical"
            >

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:orientation="horizontal"
                >

                <TextView
                    android:id="@+id/titlename"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_column="0"
                    android:text="@string/dataname" />

                <TextView
                    android:id="@+id/dataname"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_column="1"
                    android:text="TextView" />

            </TableRow>
            <View
                android:layout_height="1dp"
                android:background="#DDDDDD"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_width="match_parent" />

            <TableRow
                android:layout_width="fill_parent"

                android:layout_height="fill_parent"

                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/titlesex"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_column="0"
                    android:text="@string/datasex" />

                <TextView
                    android:id="@+id/datasex"

                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_below="@id/dataname"
                    android:layout_column="1"
                    android:text="TextView" />
            </TableRow>
            <View
                android:layout_height="1dp"
                android:background="#DDDDDD"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_width="match_parent" />

            <TableRow
                android:layout_width="fill_parent"

                android:layout_height="fill_parent"

                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/titletype"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_column="0"
                    android:text="@string/datatype" />

                <TextView
                    android:id="@+id/datatype"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_below="@id/datasex"
                    android:layout_column="1"
                    android:text="TextView" />
            </TableRow>
            <View
                android:layout_height="1dp"
                android:background="#DDDDDD"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_width="match_parent" />
            <TableRow
                android:layout_width="fill_parent"

                android:layout_height="fill_parent"

                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/titlebuild"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_column="0"
                    android:text="@string/databuild" />

                <TextView
                    android:id="@+id/databuild"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_below="@id/datatype"
                    android:layout_column="1"
                    android:text="TextView" />
            </TableRow>
            <View
                android:layout_height="1dp"
                android:background="#DDDDDD"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_width="match_parent" />
            <TableRow
                android:layout_width="fill_parent"

                android:layout_height="fill_parent"

                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/titleage"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_column="0"
                    android:text="@string/dataage" />

                <TextView
                    android:id="@+id/dataage"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_below="@id/databuild"
                    android:layout_column="1"
                    android:text="TextView" />

            </TableRow>
            <View
                android:layout_height="1dp"
                android:background="#DDDDDD"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_width="match_parent" />
            <TableRow
                android:layout_width="fill_parent"

                android:layout_height="fill_parent"

                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/titlevariety"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_column="0"
                    android:text="@string/datavariety" />

                <TextView
                    android:id="@+id/datavariety"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_below="@id/dataage"
                    android:layout_column="1"
                    android:text="TextView" />

            </TableRow>
            <View
                android:layout_height="1dp"
                android:background="#DDDDDD"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_width="match_parent" />

            <TableRow
                android:layout_width="fill_parent"

                android:layout_height="fill_parent"

                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/titlereason"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_column="0"
                    android:text="@string/datareason" />

                <TextView
                    android:id="@+id/datareason"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_below="@id/datavariety"
                    android:layout_column="1"
                    android:text="TextView" />
            </TableRow>
            <View
                android:layout_height="1dp"
                android:background="#DDDDDD"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_width="match_parent" />
            <TableRow
                android:layout_width="fill_parent"

                android:layout_height="fill_parent"

                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/titleacceptnum"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_column="0"
                    android:text="@string/dataacceptnum" />

                <TextView
                    android:id="@+id/dataacceptnum"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_below="@id/datareason"
                    android:layout_column="1"
                    android:text="TextView" />

            </TableRow>
            <View
                android:layout_height="1dp"
                android:background="#DDDDDD"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_width="match_parent" />
            <TableRow
                android:layout_width="fill_parent"

                android:layout_height="fill_parent"

                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/titlechipnum"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_column="0"
                    android:text="@string/datachipnum" />

                <TextView
                    android:id="@+id/datachipnum"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_below="@id/dataacceptnum"
                    android:layout_column="1"
                    android:text="TextView" />

            </TableRow>
            <View
                android:layout_height="1dp"
                android:background="#DDDDDD"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_width="match_parent" />
            <TableRow
                android:layout_width="fill_parent"

                android:layout_height="fill_parent"

                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/titleissterill"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_column="0"
                    android:text="@string/dataIsSterilization" />

                <TextView
                    android:id="@+id/dataissterill"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_below="@id/datachipnum"
                    android:layout_column="1"
                    android:text="TextView" />

            </TableRow>
            <View
                android:layout_height="1dp"
                android:background="#DDDDDD"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_width="match_parent" />
            <TableRow
                android:layout_width="fill_parent"

                android:layout_height="fill_parent"

                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/titlehairtype"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_column="0"
                    android:text="@string/datahairtype" />

                <TextView
                    android:id="@+id/datahairtype"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_below="@id/dataissterill"
                    android:layout_column="1"
                    android:text="TextView" />
            </TableRow>
            <View
                android:layout_height="1dp"
                android:background="#DDDDDD"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_width="match_parent" />
            <TableRow
                android:layout_width="wrap_content"

                android:layout_height="wrap_content"

                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/titlenote"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/datanote" />

                <TextView
                    android:id="@+id/datanote"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/datahairtype"
                    android:text="TextView" />

            </TableRow>
            <View
                android:layout_height="1dp"
                android:background="#DDDDDD"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_width="match_parent" />
            <TableRow
                android:layout_width="match_parent"

                android:layout_height="fill_parent"

                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/titleresettlement"
                    android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:layout_column="0"
                    android:text="@string/dataResettlement" />

                <TextView
                    android:id="@+id/dataResettlement"
                    android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:layout_below="@id/datanote"
                    android:maxLines="5"
                    android:layout_column="1"
                    android:text="TextView" />

            </TableRow>
            <View
                android:layout_height="1dp"
                android:background="#DDDDDD"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_width="match_parent" />
            <TableRow
                android:layout_width="fill_parent"

                android:layout_height="fill_parent"

                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/titlephone"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_column="0"
                    android:text="@string/dataPhone" />

                <TextView
                    android:id="@+id/dataphone"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_below="@id/dataResettlement"
                    android:layout_column="1"
                    android:text="TextView" />

            </TableRow>
            <View
                android:layout_height="1dp"
                android:background="#DDDDDD"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_width="match_parent" />
            <TableRow
                android:layout_width="fill_parent"

                android:layout_height="fill_parent"

                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/titleemail"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_column="0"
                    android:text="@string/dataemail" />

                <TextView
                    android:id="@+id/dataemail"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_below="@id/dataphone"
                    android:layout_column="1"
                    android:text="TextView" />

            </TableRow>
            <View
                android:layout_height="1dp"
                android:background="#DDDDDD"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_width="match_parent" />
            <TableRow
                android:layout_width="fill_parent"

                android:layout_height="fill_parent"

                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/titlechildreanlong"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_column="0"
                    android:text="@string/dataChildreAnlong" />

                <TextView
                    android:id="@+id/dataChildreAnlong"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_below="@id/dataemail"
                    android:layout_column="1"
                    android:text="TextView" />
            </TableRow>
            <View
                android:layout_height="1dp"
                android:background="#DDDDDD"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_width="match_parent" />
            <TableRow
                android:layout_width="fill_parent"

                android:layout_height="fill_parent"

                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/titleanimalalong"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_column="0"
                    android:text="@string/dataAnimalAnlong" />

                <TextView
                    android:id="@+id/dataAnimalAnlong"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_below="@id/dataChildreAnlong"
                    android:layout_column="1"
                    android:text="TextView" />
            </TableRow>
            <View
                android:layout_height="1dp"
                android:background="#DDDDDD"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_width="match_parent" />
            <TableRow
                android:layout_width="fill_parent"

                android:layout_height="fill_parent"

                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/titlebodyweight"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_column="0"
                    android:text="@string/dataBodyweight" />

                <TextView
                    android:id="@+id/dataBodyweight"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_below="@id/dataAnimalAnlong"
                    android:layout_column="1"
                    android:text="TextView" />

            </TableRow>
        </TableLayout>
    </android.support.v4.widget.NestedScrollView>
